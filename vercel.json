{
  "version": 2,
  "buildCommand": "node vercel-build-simple.mjs",
  "outputDirectory": "dist",
  "github": {
    "enabled": false,
    "silent": true
  },
  "framework": "vite",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}